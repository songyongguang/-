/* pages/service/result.wxss */
.resultpage{
  display: flex;
  flex-direction: column;
  margin: 5% 5%;
  justify-content: center;
}
image{
  width: 100px;
  height: 100px;
  margin: 0 auto 10% auto;
}
.resultpage view{
  width: 100%;
  margin: 0 auto;

}
.resultpage view:nth-child(1){
  display: flex;
  flex-direction: column;
  margin-top: 20%;
}
text{
  text-align: center;
}
.resultpage view:nth-child(2){
  width: 100%;
  display: flex;
  justify-content: center;
  font-size: 16px;
  color: grey;
  margin-top: 5%;
}
button{
  margin-top:5%;
  width: 90%;
  background-color: #1AAD19;
  color: white;
}